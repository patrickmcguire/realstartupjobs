<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Company</th>
    <th>Title</th>
    <th>Url</th>
    <th>Technical</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.company_id %></td>
    <td><%= job.title %></td>
    <td><%= job.url %></td>
    <td><%= job.technical %></td>
    <td><%= job.type %></td>
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
